@font-face {
	font-family:"MavenPro";
	src: url("/EBP1/font/Maven_Pro_Regular.ttf");
}
#monitor_panel{
	padding: 18px 0 0 19px;
}
#monitor_head{
	width: 1131px;
	height: 39px;
	background-color: #E74C3C;
}
#monitor_head_content{
	height: 39px;
	padding-left: 40px;
	font-size: 28px;
	color: #fff;
	background-color: #E74C3C;
}
