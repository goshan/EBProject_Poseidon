	display: inline-block;
	width: 146px;
	font-size: 12pt;
	color: #6d6d6d;
	margin: 12px 0 12px 12px;
	vertical-align: middle;
	text-align: center;
}
.monitor_all_monitor_operate{
	/*

	display: inline-block;
	text-align: center;
	vertical-align: middle;
	*/
	height: 5em;
	line-height: 5em;
	overflow: hidden;
	width: 0px;
	margin: 12px 0px 12px 32.6px;

}
.monitor_all_monitor_button{
	display: inline-block;
	width: 60px;
	height: 26px;
	margin-right: 4px;
}
.monitor_all_monitor_detail{
	background: url("/EBP1/image/monitorDetailButton.png") no-repeat 0 0;
}
.monitor_all_monitor_detail:hover{
	background: url("/EBP1/image/monitorDetailButton.png") no-repeat 0 -26px;
}
.monitor_all_monitor_detail:active{
	background: url("/EBP1/image/monitorDetailButton.png") no-repeat 0 -52px;
}
.monitor_all_monitor_ignore{
	background: url("/EBP1/image/monitorIgnoreButton.png") no-repeat 0 0;
}
