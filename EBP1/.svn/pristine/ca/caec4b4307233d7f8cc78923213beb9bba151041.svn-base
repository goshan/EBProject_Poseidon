.monitor_all_monitor_fans{
	display: inline-block;
	width: 85px;
	font-size: 12pt;
	color: #6d6d6d;
	margin: 12px 0 12px 12px;
	vertical-align: middle;
}
.monitor_all_monitor_text{
	display: inline-block;
	width: 520px;
	font-size: 12pt;
	color: #6d6d6d;
	margin: 12px 0 12px 12px;
	vertical-align: middle;
}
