#membership_cover{
	position: absolute;
	top: 0;
	left: 0;
	width: 1366px;
	height: 650px;
	background-color: #000;
	opacity: 0.5;
	z-index: 120;
}


#membership_panel{
	padding: 18px 0 0 34px;
}
.membership_select_tag{
	display: inline-block;
	width: 320px;
	height: 75px;
	position: relative;
}
#membership_verify{
	background-color: #e67e22;
	
}
#membership_manage{
	background-color: #dbdbdb;
	margin-left: -5px;
	
}
.membership_select_num{
	display: inline-block;
	font-size: 22px;
	color: #fff;
	text-align: center;
	position: absolute;
	top: 15px;
}
#membership_verify_num{
	width: 115px;
	opacity: 0;
}
#membership_manage_num{
	width: 70px;
	opacity: 1;
}
.membership_select_icon{
	position: absolute;
}
#membership_verify_icon{
	top: 0;
	left: 0;
	opacity: 1;
}
#membership_manage_icon{
	top: 0;
	left: 90px;
	opacity: 0.5;
}
.membership_select_title{
	position: absolute;
	top: 16px;
	right: 14px;
	font-size: 36px;
}
#membership_verify_title{
	color: #fff;
}
#membership_manage_title{
	color: #a09c97;
}
.membership_select_arrow{
	position: absolute;
	top: 75px;
	left: 150px;
}
#membership_verify_arrow{
	opacity: 1;
}
#membership_manage_arrow{
	opacity: 0;
}
#membership_table_title{
	width: 1091px;
	height: 39px;
	margin-top: 7px;
	font-size: 22px;
	color: #fff;
	padding: 13px 0 0 40px;
	background-color: #efbb95;
	margin-left: -15px;
}
#membership_table_corner{
	position: absolute;
	top: 241px;
	left: 195px;
}
.membership_table_tr{
	width: 1116px;
	height: 40px;
}
#membership_table_attrname{
	background-color: #f9e3d4;
}
.membership_table_evenrow{
	background-color: #fff;
}
.membership_table_oddrow{
	background-color: #f4f4f4;
}
#membership_table_attrname span, .membership_table_oddrow span, .membership_table_evenrow span{
	display: inline-block;
	height: 40px;
	line-height: 40px;
}
#membership_table_attrname span{
	font-size: 16px;
	color: #515151;
	margin-right: -5px;
}
.membership_table_oddrow span, .membership_table_evenrow span{
	font-size: 15px;
	color: #727272;
	vertical-align: top;
}
.membership_username_column{
	width: 153px;
	padding-left: 52px;
}
.membership_realname_column{
	width: 144px;
	padding-left: 10px;
}
.membership_usertype_column{
	width: 112px;
	padding-left: 10px;
}
.membership_email_column{
	width: 205px;
	padding-left: 10px;
}
.membership_remarks_column{
	width: 205px;
	padding-left: 10px;
}
.membership_option_column{
	width: 195px;
	padding-left: 10px;
	position: relative;
}
.membership_verify_button{
	display: inline-block;
	width: 42px;
	height: 24px;
	margin-top: 8px;
	background: url("/EBP1/image/membershipVerifyButton.png") no-repeat 0 0;
}
.membership_verify_button:hover{
	background: url("/EBP1/image/membershipVerifyButton.png") no-repeat 0 -24px;
}
.membership_verify_button:active{
	background: url("/EBP1/image/membershipVerifyButton.png") no-repeat 0 -48px;
}
.membership_edit_button{
	display: inline-block;
	width: 42px;
	height: 24px;
	margin-top: 8px;
	background: url("/EBP1/image/membershipEditButton.png") no-repeat 0 0;
}
.membership_edit_button:hover{
	background: url("/EBP1/image/membershipEditButton.png") no-repeat 0 -24px;
}
.membership_edit_button:active{
	background: url("/EBP1/image/membershipEditButton.png") no-repeat 0 -48px;
}
.membership_delete_button{
	display: inline-block;
	width: 42px;
	height: 24px;
	margin: 8px 0 0 1px;
	background: url("/EBP1/image/membershipDeleteButton.png") no-repeat 0 0;
}
.membership_delete_button:hover{
	background: url("/EBP1/image/membershipDeleteButton.png") no-repeat 0 -24px;
}
.membership_delete_button:active{
	background: url("/EBP1/image/membershipDeleteButton.png") no-repeat 0 -48px;
}
.membership_verify_tip, .membership_edit_tip{
	position: absolute;
	right: 195px;
	top: 7px;
}
#membership_table_body{
	width: 1116px;
	height: 320px;
}





#membership_edit_panel{
	position: absolute;
	top: 140px;
	left: 408px;
	width: 550px;
	height: 370px;
	z-index: 120;
	background-color: #fff;
}
#membership_edit_head{
	width: 550px;
	height: 45px;
	background-color: #e67e22;
}
#membership_edit_head_img{
	width: 80px;
	height: 45px;
	margin-left: 235px;
}
#membership_edit_close{
	display: inline-block;
	width: 	27px;
	height: 26px;
	background: url("/EBP1/image/membershipEditCloseButton.png") no-repeat center;
	margin: 0 0 9px 196px;
}
#membership_edit_body{
	width: 483px;
	height: 308px;
	padding: 17px 0 0 67px;
}
.membership_edit_line{
	width: 416px;
	height: 34px;
	margin-bottom: 8px;
}
.membership_edit_label{
	display: inline-block;
	width: 134px;
	height: 34px;
	background-color: #bfbfbf;
	font-size: 16px;
	color: #fff;
	line-height: 34px;
	padding-left: 8px;
}
.membership_edit_input{
	width: 269px;
	height: 34px;
	font-family: MavenPro, Microsoft Yahei;
	font-size: 16px;
	color: #bfbfbf;
}
#membership_edit_button_line{
	width: 416px;
	height: 28px;
	margin-top: 18px;
}
#membership_edit_reset{
	display: block;
	width: 84px;
	height: 28px;
	background: url("/EBP1/image/membershipEditResetButton.png") no-repeat 0 0;
	float: left;
}
#membership_edit_reset:hover{
	background: url("/EBP1/image/membershipEditResetButton.png") no-repeat 0 -28px;
}
#membership_edit_reset:active{
	background: url("/EBP1/image/membershipEditResetButton.png") no-repeat 0 -56px;
}
#membership_edit_save{
	display: block;
	width: 80px;
	height: 28px;
	background: url("/EBP1/image/membershipEditSaveButton.png") no-repeat 0 0;
	float: right;
}
#membership_edit_save:hover{
	background: url("/EBP1/image/membershipEditSaveButton.png") no-repeat 0 -28px;
}
#membership_edit_save:active{
	background: url("/EBP1/image/membershipEditSaveButton.png") no-repeat 0 -56px;
}
