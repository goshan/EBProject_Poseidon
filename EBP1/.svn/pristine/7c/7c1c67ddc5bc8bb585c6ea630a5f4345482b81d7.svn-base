package com.ibm.p1.entity;


import java.util.Date;
import java.util.List;

public class TotalKeyWords {
	private String keyWord;
	private int totalAppear;
	private float totalWeight;
	private List<Date> appearDates;
	
	public int getTotalAppear() {
		return totalAppear;
	}
	public void setTotalAppear(int totalAppear) {
		this.totalAppear = totalAppear;
	}
	public String getKeyWord() {
		return keyWord;
	}
	public void setKeyWord(String keyWord) {
		this.keyWord = keyWord;
	}
	public float getTotalWeight() {
		return totalWeight;
	}
	public void setTotalWeight(float totalWeight) {
		this.totalWeight = totalWeight;
	}
	public List<Date> getAppearDates() {
		return appearDates;
	}
	public void setAppearDates(List<Date> appearDates) {
		this.appearDates = appearDates;
	}
	
	
}
