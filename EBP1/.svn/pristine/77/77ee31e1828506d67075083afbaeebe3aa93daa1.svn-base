#panel{
	height: 570px;
}
.bg{
	left: 0;
	width: 1366px;
	height: 570px;
	position: absolute;
}
#content{
	height: 570px;
	width: 960px;
	margin: 0 auto;
	position: relative;
}
#logo{
	width: 265px;
	margin: 0 auto;
	padding-top: 57px;
}
#intro{
	position: absolute;
	top: 180px;
	left: 15px;
	font-size: 55px;
	font-family: MavenProLight300;
	color: #fff;
	margin-top: 10px;
}
#logo_big{
	position: absolute;
	top: 357px;
	left: 15px;
}
#line{
	width: 2px;
	height: 386px;
	background-color: #fff;
	opacity: 0.3;
	position: absolute;
	left: 479px;
	top: 113px;
}
#loading{
	position: absolute;
	left: 464px;
	top: 82px;
}
#ibm_logo{
	width: 71px;
	height: 71px;
	position: absolute;
	bottom: -25px;
	left: 444px;
}
#login_form{
	width: 345px;
	height: 300px;
	position: absolute;
	right: 7px;
	top: 127px;
}
#signup_form{
	width: 296px;
	height: 300px;
	position: absolute;
	right: 7-50px;
	top: 115px;
}
#login_error_tip{
	width: 281px;
	height: 31px;
	margin-bottom: 10px;
	background: url("/EBP1/image/wrongMessage.png") no-repeat center;
	font-size: 16px;
	color: #ff8d40;
	text-align: center;
	line-height: 30px;
	
}
#signup_error_tip{
	width: 236px;
	height: 31px;
	margin-bottom: 10px;
	background: url("/EBP1/image/wrongMessage.png") no-repeat center;
	font-size: 16px;
	color: #ff8d40;
	text-align: center;
	line-height: 30px;
	
}
.switch{
	margin-bottom: 8px;
}
#login_label{
	margin-right: 152px;
}
#signup_label{
	margin-right: 103px;
}
.switch_label{
	font-size: 16px;
	color: #fff;
}
.switch_button{
	font-size: 16px;
	color: #fff;
}
.switch_button img{
	padding-left: 10px;
}
.login_form_line{
	margin-bottom: 19px;
}
.signup_form_line{
	margin-bottom: 12px;
}
#login_username_input{
	background: url("/EBP1/image/loginUserName.png") no-repeat 0 0;
}
#login_username_input:focus{
	background: url("/EBP1/image/loginUserName.png") no-repeat 0 -49px;
}
#login_password_input{
	background: url("/EBP1/image/loginPassword.png") no-repeat 0 0;
}
#login_password_input:focus{
	background: url("/EBP1/image/loginPassword.png") no-repeat 0 -49px;
}
.login_input{
	height: 50px;
	width: 283px;
	border: none;
	line-height: 50px;
	font-family: MavenPro, Microsoft Yahei;
	font-size: 20px;
	color: #ccc;
	padding: 0 65px;
	
}
.signup_input{
	height: 41px;
	width: 235px;
	border: none;
	line-height: 41px;
	font-size: 20px;
	font-family: MavenPro, Microsoft Yahei;
	color: #ccc;
	padding: 0 20px;
	background: url("/EBP1/image/signupInput.png") no-repeat 0 0;
}
.signup_input:focus{
	background: url("/EBP1/image/signupInput.png") no-repeat 0 -41px;
}
.input_error{
	vertical-align: middle;
	margin: -10px 0 0 -5px;
}
#login_submit{
	display: block;
	height: 50px;
	width: 283px;
	background: url("/EBP1/image/loginButton.png") no-repeat 0 0;
}
#login_submit:hover{
	background: url("/EBP1/image/loginButton.png") no-repeat 0 -49px;
}
#login_submit:active{
	background: url("/EBP1/image/loginButton.png") no-repeat 0 -98px;
}
#signup_submit{
	display: block;
	height: 41px;
	width: 235px;
	background: url("/EBP1/image/signupButton.png") no-repeat 0 0;
}
#signup_submit:hover{
	background: url("/EBP1/image/signupButton.png") no-repeat 0 -41px;
}
#signup_submit:active{
	background: url("/EBP1/image/signupButton.png") no-repeat 0 -82px;
}
#clock_bg{
	height: 80px;
	width: 1366px;
	background-color: #2c3b4c;
}
#clock_content{
	height: 80px;
	width: 960px;
	margin: 0 auto;
	position: relative;
}
#date{
	position: absolute;
	left: 32px;
	bottom: 13px;
}
#time{
	position: absolute;
	right: 37px;
	bottom: 10px;
}
.clock{
	font-size: 55px;
	font-family: MissionGothicLight;
	color: #fff;
	opacity: 0.3;
}
.list{
	position: absolute;
}